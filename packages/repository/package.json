    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "copyfiles": "^2.4.1",
    "prisma": "^4.6.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1"
  }
}
