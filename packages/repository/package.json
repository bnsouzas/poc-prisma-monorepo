{
  "name": "@bnsouzastests/repository",
  "version": "1.0.11",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn generate",
    "build": "yarn run clean && tsc",
